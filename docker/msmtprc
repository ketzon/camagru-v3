defaults
auth           on
tls            on
tls_starttls   on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        /tmp/msmtp.log

account gmail
host     smtp.gmail.com
port     587
from     ketzon.contact@gmail.com
user     ketzon.contact@gmail.com
passwordeval "printenv SMTP_PASS"

account default : gmail
